{
    "name": "Zoom Auto Join",
    "version": "1.0",
    "description": "An extension that automatically joins scheduled Zoom calls for you.",
    "manifest_version": 2,
    "permissions": ["alarms", "tabs", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_icon": "smile.png",
        "default_popup": "popup.html"
    }
}